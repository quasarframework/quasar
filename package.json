{
  "name": "quasar-framework",
  "version": "0.14.0",
  "description": "Simultaneously build desktop/mobile SPA websites & phone/tablet apps with VueJS",
  "main": "dist/quasar.esm.js",
  "module": "dist/quasar.esm.js",
  "jsnext:main": "dist/quasar.esm.js",
  "files": [
    "dist",
    "src",
    "animations"
  ],
  "scripts": {
    "clean": "node build/script.clean.js",
    "dev": "node build/script.dev.js",
    "build": "node build/script.build.js",
    "lint": "eslint --ext .js,.vue src dev",
    "deploy": "npm run build simple && rm -rf deploy && mkdir deploy && cp -R dist src LICENSE package.json deploy && cp build/README-edge.md deploy/README.md && cp .gitignore deploy/.gitignore",
    "publish": "npm run build simple && rm -rf deploy && mkdir deploy && cp -R dist src LICENSE package.json deploy && cp build/README-edge.md deploy/README.md && cp .gitignore deploy/.gitignore && sh node_modules/git-directory-deploy/bin/git-directory-deploy.sh -ddeploy -bmaster -rhttps://github.com/pdanpdan/quasar.git && rm -rf deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdanpdan/quasar.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "quasar",
    "js",
    "phone",
    "tablet",
    "desktop",
    "spa",
    "website",
    "electron"
  ],
  "author": {
    "name": "Razvan Stoenescu",
    "email": "razvan.stoenescu@gmail.com",
    "url": "http://quasar-framework.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rstoenescu/quasar-framework/issues"
  },
  "homepage": "http://quasar-framework.org",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.17.0",
    "colors": "^1.1.2",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.3.1",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.15.1",
    "opn": "^4.0.2",
    "parse5": "^2.1.0",
    "postcss-loader": "^1.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "roboto-fontface": "^0.7.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-vue": "2.3.1",
    "shelljs": "^0.7.0",
    "stylint": "^1.5.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.7",
    "vue": "^2.2.6",
    "vue-loader": "11.1.4",
    "vue-router": "^2.0.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "vue": "^2.2.0"
  }
}
