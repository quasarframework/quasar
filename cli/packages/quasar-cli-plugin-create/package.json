{
  "name": "@quasar/cli-plugin-create",
  "version": "1.0.0-alpha.2",
  "description": "Quasar Framework CLI Create Plugin",
  "bin": {
    "quasar-create": "./index.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "quasar",
    "cli",
    "web",
    "spa",
    "pwa",
    "electron",
    "vue",
    "vuejs",
    "cordova"
  ],
  "author": {
    "name": "Razvan Stoenescu",
    "email": "razvan.stoenescu@gmail.com",
    "url": "https://github.com/quasarframework"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quasarframework/quasar"
  },
  "license": "MIT",
  "bugs": "https://github.com/quasarframework/quasar/issues",
  "homepage": "http://quasar-framework.org",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "dependencies": {
    "@quasar/cli": "../../",
    "@quasar/cli-helpers": "../quasar-cli-helpers/",
    "chalk": "^2.4.1",
    "inquirer": "^6.2.1",
    "ora": "^3.0.0",
    "rimraf": "^2.6.2",
    "tildify": "^1.2.0",
    "user-home": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/index.{js}"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "testMatch": [
      "<rootDir>/__test__/**/?(*.)(spec).js?(x)",
      "<rootDir>/__test__/**/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "resolver": null,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
