{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "productName": "<%= productName %>",
  "author": "<%= author %>",
  "private": true,
  "scripts": {
    <% if (preset.lint) { %>"lint": "eslint --ext .js,.ts,.vue ./",<% } %>
    <% if (lintConfig === 'prettier') { %>"format": "prettier --write \"**/*.{js,ts,vue,<% if (css !== 'sass') { %><%= css %><% } %>,html,md,json}\" --ignore-path .gitignore",<% } %>
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    <% if (typescriptConfig === 'class') { %>"vue-class-component": "^8.0.0-rc.1",<% } %>
    <% if (preset.axios) { %>"axios": "^0.21.1",<% } %>
    <% if (preset.i18n) { %>"vue-i18n": "^9.0.0",<% } %>
    <% if (preset.pinia) { %>"pinia": "^2.0.11",<% } %>
    <% if (preset.vuex) { %>"vuex": "^4.0.1",<% } %>
    "@quasar/extras": "^1.0.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    <% if (preset.lint) { %>
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.0.0",
    <% if (lintConfig === 'standard') { %>
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    <% } %>
    <% if (lintConfig === 'airbnb') { %>
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    <% } %>
    <% if (lintConfig === 'prettier') { %>
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    <% } %>
    <% } %>
    "@types/node": "^12.20.21",
    "@quasar/app-vite": "^1.0.0-alpha.0",
    "autoprefixer": "^10.4.2",
    "typescript": "^4.5.4",
    "vue-tsc": "^0.29.8"
  },
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
