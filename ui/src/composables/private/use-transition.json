{
  "props": {
    "transition-show": {
      "extends": "transition",
      "default": "fade"
    },

    "transition-hide": {
      "extends": "transition",
      "default": "fade"
    },

    "transition-duration": {
      "type": [ "String", "Number" ],
      "desc": "Transition duration (in milliseconds, without unit)",
      "default": 300,
      "examples": [ 500, "1200" ],
      "category": "transition"
    }
  }
}
