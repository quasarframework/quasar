{
  "meta": {
    "docsUrl": "https://v1.quasar.dev/quasar-plugins/addressbar-color"
  },

  "injection": "$q.addressbarColor",

  "methods": {
    "set": {
      "desc": "Sets addressbar color (for browsers that support it)",
      "params": {
        "hexColor": {
          "type": "String",
          "desc": "Color in hex format",
          "required": true,
          "examples": [ "#ff0000" ]
        }
      }
    }
  }
}
