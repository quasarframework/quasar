{
  "meta": {
    "docsUrl": "https://v1.quasar.dev/vue-components/toolbar"
  },

  "behavior": {
    "$listeners": true
  },

  "props": {
    "shrink": {
      "type": "Boolean",
      "desc": "By default, QToolbarTitle is set to grow to the available space. However, you can reverse that with this prop",
      "category": "behavior"
    },
    "align": {
      "type": "String",
      "desc": "Align the text inside the QToolbarTitle",
      "link": "https://developer.mozilla.org/it/docs/Web/CSS/text-align",
      "examples": [
        "center",
        "left",
        "right",
        "justify",
        "justify-all"
      ]
    }
  },

  "slots": {
    "default": {
      "extends": "default"
    }
  }
}
