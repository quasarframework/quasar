{
  "mixins": [ "mixins/size" ],

  "behavior": {
    "$listeners": true
  },

  "props": {
    "color": {
      "extends": "color"
    },

    "thickness": {
      "type": "Number",
      "desc": "Override value to use for stroke-width",
      "default": 5,
      "examples": [ "2", "5" ],
      "category": "style"
    }
  }
}
